  <script src="index.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.js"></script>
  <script id="menu-template" type="text/x-handlebars-template">
  <div>
    <h1>{{name}}</h1>
    <ul class="body">
      {{#each menu}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
  </div>
  </script>
